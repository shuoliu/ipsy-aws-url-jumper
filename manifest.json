{
  "manifest_version": 2,

  "name": "Ipsy AWS URL Jumper",
  "description": "This extension redirect aws urls to the right urls.",
  "version": "1.0",

  "permissions": [
    "tabs"
  ],

  "background": {
    "scripts": [ "background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "AWS URL Jumper"
  }
}

